{
    "name": "speicher210/fastbill-api",
    "type": "library",
    "description": "API for Fastbill",
    "keywords": [
        "speicher210",
        "fastbill"
    ],
    "homepage": "https://github.com/Speicher210/fastbill-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Dragos Protung",
            "email": "dragos@protung.ro",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "guzzlehttp/guzzle": "~6.1",
        "jms/serializer": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "~5.1",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Speicher210\\Fastbill\\Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Speicher210\\Fastbill\\Test\\Api\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
